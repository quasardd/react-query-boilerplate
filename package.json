{
  "name": "react-query-restful",
  "version": "0.3.3",
  "description": "Just a small collection of hooks that make it easy to integrate React Query into a REST API.",
  "repository": {
    "type": "git",
    "url": "https://github.com/quasardd/react-query-restful/"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Caio Henrique <chcp.caio@gmail.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc -p tsconfig-build.json",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs -p tsconfig-build.json",
    "types": "tsc --noEmit --declaration",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.3",
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "type-fest": "^2.12.2"
  },
  "devDependencies": {
    "@swc/core": "^1.2.174",
    "@swc/jest": "^0.2.20",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.31",
    "@types/react": "18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "axios-mock-adapter": "^1.20.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^40.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-query": "^3.38.1",
    "react-test-renderer": "^18.1.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "react": "^18.1.0",
    "react-query": "^3.38.1"
